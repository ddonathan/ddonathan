{
  "name": "docxtemplater-image-module",
  "version": "3.22.0",
  "private": true,
  "description": "Image Module for docxtemplater",
  "main": "js/index.js",
  "scripts": {
    "test:coverage": "istanbul cover _mocha --  es6/test.js",
    "compile": "rimraf js && mkdirp js && babel es6 --out-dir js",
    "preversion": "rimraf js build browser && npm test && npm run ts-self-test && npm run browserify && npm run test:chrome && npm run test:typings",
    "test:compiled": "mocha --max-old-space-size=160 js/test.js",
    "test:es6": "mocha --max-old-space-size=160 es6/test.js",
    "lint": "eslint --report-unused-disable-directives . --cache",
    "ts-self-test": "MODULE_NAME=ImageModule ../../scripts/convert-ts.bash",
    "lint:fix": "eslint --report-unused-disable-directives . --fix --cache",
    "test:firefox": "BROWSER=FIREFOX ./webdriver.bash",
    "test:chrome": "BROWSER=CHROME ./webdriver.bash",
    "test:typings": "cp es6/*.ts js && tsd .",
    "test": "npm run compile && npm run test:compiled",
    "browserify:test": "FILE=test webpack",
    "browserify:lib": "OUTPUT_PATH=imagemodule LIB_NAME=ImageModule webpack",
    "browserify:min": "MIN=true npm run browserify:lib",
    "browserify": "npm run browserify:lib && npm run browserify:min && npm run browserify:test",
    "test:es6:update-fixtures": "UPDATE=true mocha --max-old-space-size=160 --full-trace --check-leaks es6/test.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@wdio/cli": "^8.12.2",
    "@xmldom/xmldom": "^0.8.9",
    "angular-expressions": "^1.1.9",
    "babel-loader": "^9.1.3",
    "browserify": "^17.0.0",
    "chai": "^4.3.7",
    "chalk": "^5.3.0",
    "eslint_d": "^12.2.1",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-import": "^2.27.5",
    "exif": "^0.6.0",
    "finalhandler": "^1.2.0",
    "image-size": "^1.0.2",
    "istanbul": "^0.4.5",
    "mkdirp": "^3.0.1",
    "mocha": "^10.2.0",
    "pizzip": "^3.1.4",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "selenium-standalone": "^8.3.0",
    "serve-static": "^1.15.0",
    "tsd": "^0.28.1",
    "webdriverio": "^8.12.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "author": "Edgar Hipp",
  "license": "SEE LICENSE IN https://docxtemplater.com/PRO-LICENSE",
  "types": "./js/index.d.ts",
  "dependencies": {
    "docxtemplater": "^3.37.12"
  }
}
